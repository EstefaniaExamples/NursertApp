{
  "name": "children-api",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.yml",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "jest --config ./jest.config.ts",
    "start": "./node_modules/.bin/serverless offline start -s dev",
    "debug": "export SLS_DEBUG=* && node --inspect ./node_modules/.bin/serverless offline -s dev",
    "clean": "shx rm -rf dist/*",
    "build": "npm run clean && tsc --project tsconfig.json"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.224.0",
    "@aws-sdk/lib-dynamodb": "^3.224.0",
    "@aws-sdk/types": "^3.224.0 ",
    "aws-lambda": "^1.0.7",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@serverless/typescript": "^3.25.0",
    "@types/aws-lambda": "^8.10.109",
    "@types/jest": "^29.2.4",
    "@types/jest-when": "^3.5.2",
    "@types/node": "^18.11.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "aws-sdk-client-mock": "^2.0.1",
    "esbuild": "^0.15.18",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.3.1",
    "jest-when": "^3.5.2",
    "prettier": "^2.8.0",
    "serverless": "^3.25.1",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-esbuild": "^1.33.2",
    "serverless-offline": "^12.0.0",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-typescript": "^2.1.4",
    "shx": "^0.3.4",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^4.1.1",
    "tsconfig-paths-jest": "^0.0.1",
    "tslog": "^4.4.4",
    "typescript": "^4.9.3"
  },
  "author": "Estefan√≠a Castro Vizoso",
  "license": "MIT"
}
