{
  "name": "children-api",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.yml",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "jest --config ./jest.config.ts",
    "start": "./node_modules/.bin/serverless offline -s dev",
    "debug": "export SLS_DEBUG=* && node --inspect ./node_modules/.bin/serverless offline -s dev"
  },
  "engines": {
    "node": ">=16.16.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.170.0",
    "@aws-sdk/lib-dynamodb": "^3.170.0",
    "@aws-sdk/types": "^3.170.0",
    "@middy/core": "^2.5.3",
    "@middy/http-json-body-parser": "^2.5.3",
    "aws-lambda": "^1.0.7",
    "middy": "^0.24.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.104",
    "@types/jest": "^29.0.3",
    "@types/node": "^14.14.25",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "esbuild": "^0.14.54",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.0.3",
    "jest-cli": "^29.0.3",
    "json-schema-to-ts": "^1.5.0",
    "prettier": "^2.7.1",
    "serverless": "^3.0.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-esbuild": "^1.32.8",
    "serverless-offline": "^10.1.0",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-typescript": "^2.1.2",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.8.3"
  },
  "author": "Estefan√≠a Castro Vizoso",
  "license": "MIT"
}
